<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.true123.youhui.mapper.UserMapper">
	<select id="selectId" resultType="cn.true123.youhui.bean.Tag">
		select * from tag where id = #{id}
	</select>
	<insert id="insertTag" parameterType="cn.true123.youhui.bean.Tag">
		insert into tag(id, name, password)
 		 values (#{id}, #{name}, #{password})
	</insert>
</mapper>